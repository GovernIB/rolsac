<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from EnllacQueryServiceEJBRemoteService WSDL service -->

  <service name="EnllacWS" provider="java:RPC" style="rpc" use="encoded">
      <parameter name="wsdlTargetNamespace" value="https://localhost:8443/sacws-api/services/EnllacWS"/>
      <parameter name="wsdlServiceElement" value="EnllacQueryServiceEJBRemoteService"/>
      <parameter name="schemaUnqualified" value="http://procediment.v2.api.rolsac.caib.es,http://fitxa.v2.api.rolsac.caib.es,http://general.v2.api.rolsac.caib.es"/>
      <parameter name="wsdlServicePort" value="EnllacWS"/>
      <parameter name="className" value="localhost.sacws_api.services.EnllacWS.EnllacWSSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="EnllacQueryServiceEJBRemote"/>
      <parameter name="typeMappingVersion" value="1.1"/>
      <operation name="obtenirProcediment" qname="operNS:obtenirProcediment" xmlns:operNS="http://intf.ejb.enllac.v2.api.rolsac.caib.es" returnQName="obtenirProcedimentReturn" returnType="rtns:ProcedimentDTO" xmlns:rtns="http://procediment.v2.api.rolsac.caib.es" soapAction="" >
        <parameter qname="in0" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="obtenirFitxa" qname="operNS:obtenirFitxa" xmlns:operNS="http://intf.ejb.enllac.v2.api.rolsac.caib.es" returnQName="obtenirFitxaReturn" returnType="rtns:FitxaDTO" xmlns:rtns="http://fitxa.v2.api.rolsac.caib.es" soapAction="" >
        <parameter qname="in0" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="obtenirProcediment obtenirFitxa"/>

      <typeMapping
        xmlns:ns="http://fitxa.v2.api.rolsac.caib.es"
        qname="ns:FitxaDTO"
        type="java:es.caib.rolsac.api.v2.fitxa.FitxaDTO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://general.v2.api.rolsac.caib.es"
        qname="ns:EntitatRemota"
        type="java:es.caib.rolsac.api.v2.general.EntitatRemota"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://procediment.v2.api.rolsac.caib.es"
        qname="ns:ProcedimentDTO"
        type="java:es.caib.rolsac.api.v2.procediment.ProcedimentDTO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"

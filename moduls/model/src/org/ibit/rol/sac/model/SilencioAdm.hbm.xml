<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.SilencioAdm" table="RSC_SILADM">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="SIL_CODI">
			<generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

        <map name="traducciones" cascade="all" table="RSC_TRASIL">
            <cache usage="read-write"/>
            <key column="TSI_CODSIL" foreign-key="RSC_TSISIL_FK"/>
            <index column="TSI_CODIDI" type="string" length="2"/>
            <composite-element class="org.ibit.rol.sac.model.TraduccionSilencio">
                <property name="nombre" length="256" column="TSI_NOMBRE"/>
                <property name="descripcion" length="4000" column="TSI_DESCRI"/>
            </composite-element>
        </map>

    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.GrupoGenericoSuscripcion" table="RSC_GRPGEN">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="GRP_COD">
            <generator class="sequence">
                <param name="sequence">RSC_SEQGRP</param>
            </generator>
        </id>
        
        <property name="tipo" column="GRP_TIPO"/>
                

        <many-to-one name="tipoSuscripcion" not-null="true" column="GRP_STPCOD"  foreign-key="FK_RSC_GRPG_REFERENCE_RSC_SCRT"/>                
        
        
        <map name="traducciones" cascade="all" table="RSC_GRPGID">
            <cache usage="read-write"/>
            <key column="SGR_GRPCOD" foreign-key="FK_RSC_GRPG_REFERENCE_RSC_GRPG"/>
            <index column="SGR_CODIDI" type="string" length="2"/>
            <composite-element class="org.ibit.rol.sac.model.TraduccionGrupoGenericoSuscripcion">
                <property name="nombre" length="100" column="SGR_NOM"/>
            </composite-element>
        </map>
    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.IconoFamilia" table="RSC_ICOFAM">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="ICF_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

        <many-to-one name="familia" cascade="none" column ="ICF_CODFAM" foreign-key="RSC_ICFFAM_FK"/>
        <many-to-one name="perfil" cascade="none" column="ICF_CODPEC" foreign-key="RSC_ICFPEC_FK"/>
        <many-to-one name="icono" cascade="all" column="ICF_ICONO" foreign-key="RSC_ICFICO_FK"/>
    </class>
</hibernate-mapping>
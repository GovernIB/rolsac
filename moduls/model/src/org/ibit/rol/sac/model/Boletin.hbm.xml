<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.Boletin" table="RSC_BOLETI">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="BOL_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>
        <property name="nombre" length="256" column="BOL_NOMBRE"/>
        <property name="enlace" length="512" column="BOL_ENLACE"/>
        <set name="normativas" cascade="none" inverse="true" lazy="true">
            <key column="NOR_CODBOL" foreign-key="RSC_NORBOL_FK"/>
            <one-to-many class="org.ibit.rol.sac.model.Normativa"/>
        </set>
    </class>
</hibernate-mapping>
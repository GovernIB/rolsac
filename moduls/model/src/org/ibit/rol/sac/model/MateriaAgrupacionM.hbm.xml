<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.MateriaAgrupacionM" table="RSC_MATAGR">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column= "MAG_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

        <many-to-one name="materia" column="MAG_CODMAT" foreign-key="RSC_MAGMAT_FK"/>
        <many-to-one name="agrupacion" column="MAG_CODAGM" foreign-key="RSC_MAGAGM_FK"/>
        <property name="orden" column= "MAG_ORDEN"/>

    </class>
</hibernate-mapping>
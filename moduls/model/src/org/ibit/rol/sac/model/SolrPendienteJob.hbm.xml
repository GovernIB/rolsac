<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.SolrPendienteJob" table="RSC_SOLJOB" mutable="true">
        <id name="id" unsaved-value="null" column="JOB_ID">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>
        <property name="fechaIni"   column="JOB_FECINI" />
        <property name="fechaFin"   column="JOB_FECFIN" />
        <property name="fechaFicha"   column="JOB_FECFCH" /> 
        <property name="fechaProcedimiento"   column="JOB_FECPRO" />
        <property name="fechaNormativa"   column="JOB_FECNOR" />
        <property name="fechaTramite"   column="JOB_FECTRA" />
        <property name="fechaUnidadAdministrativa"   column="JOB_FECUNA" />
        <property name="totalFicha" column="JOB_TOTFCH" />
    	<property name="totalFichaDoc" column="JOB_TOTDFC" />
    	<property name="totalProcedimiento" column="JOB_TOTPRO" />
    	<property name="totalProcedimientoDoc" column="JOB_TOTDPR" />
    	<property name="totalNormativa" column="JOB_TOTNOR" />
    	<property name="totalNormativaDoc" column="JOB_TOTDNO" />
    	<property name="totalTramite" column="JOB_TOTTRA" />
    	<property name="totalUnidadAdministrativa" column="JOB_TOTUNA" />
        
        
    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="org.ibit.rol.sac.model.Enlace" table="RSC_ENLACE">
    
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="ENL_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

  		<many-to-one name="ficha" column="ENL_CODFIC" foreign-key="RSC_ENLFIC_FK"/>
        <many-to-one name="procedimiento" column="ENL_CODPRO" foreign-key="RSC_ENLPRO_FK"/>
  
		<property name="orden" column="ENL_ORDEN"/>

        <map name="traducciones" cascade="all" table="RSC_TRAENL">
            <cache usage="read-write"/>
            <key column="TEN_CODENL" foreign-key="RSC_TENENL_FK"/>
            <index column="TEN_CODIDI" type="string" length="2"/>
            <composite-element class="org.ibit.rol.sac.model.TraduccionEnlace">
                <property name="titulo" length="256" column="TEN_TITULO"/>
                <property name="enlace" length="512" column="TEN_ENLACE"/>
            </composite-element>
        </map>

    </class>
    
</hibernate-mapping>
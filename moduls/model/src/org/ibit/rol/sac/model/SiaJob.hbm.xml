<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.SiaJob" table="RSC_SIAJOB" mutable="true">
        <id name="id" unsaved-value="null" column="SIJ_ID">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>
        <property name="fechaIni"       column="SIJ_FECINI" />
        <property name="fechaFin"       column="SIJ_FECFIN" />
        <property name="descBreve"  	column="SIJ_DESBRE" type="text" />
        <property name="descripcion"    column="SIJ_DESCRI" type="text" />
        <property name="estado"       	column="SIJ_ESTADO" />
        <property name="tipo"       	column="SIJ_TIPO" />

    </class>
</hibernate-mapping>
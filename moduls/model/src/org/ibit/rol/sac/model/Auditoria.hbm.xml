<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.Auditoria" table="RSC_AUDITO">
        <id name="id" unsaved-value="null" column="AUD_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

        <property name="usuario" length="256" column="AUD_USUARI"/>
        <property name="fecha" column="AUD_FECHA"/>
        <many-to-one name="historico" column="AUD_CODHIS" foreign-key="RSC_AUDHIS_FK"/>
        <property name="codigoOperacion" column="AUD_CODOPE"/>
    </class>
</hibernate-mapping>
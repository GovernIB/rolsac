<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.TipoAfectacion" table="RSC_TIPAFE">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="TIA_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

        <map name="traducciones" cascade="all" table="RSC_TRATIA">
            <cache usage="read-write"/>
            <key column="TTA_CODTIA" foreign-key="RSC_TTATIA_FK"/>
            <index column="TTA_CODIDI" type="string" length="2"/>
            <composite-element class="org.ibit.rol.sac.model.TraduccionTipoAfectacion">
                <property name="nombre" length="256" column="TTA_NOMBRE"/>
            </composite-element>
        </map>
    </class>
</hibernate-mapping>
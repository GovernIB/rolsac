<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.HechoVitalAgrupacionHV" table="RSC_HEVIAG">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column= "HVA_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

        <many-to-one name="hechoVital" column="HVA_CODHEV" foreign-key="RSC_HVAHEV_FK"/>
        <many-to-one name="agrupacion" column="HVA_CODAGH" foreign-key="RSC_HVAAGH_FK"/>
        <property name="orden" column= "HVA_ORDEN"/>

    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.TipoSuscripcion" table="RSC_SCRTIP">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="STP_COD">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>
        
        <property name="unidadAdministrativa" column="STP_CODUNI"/>        
        <property name="identificador" length="10"  column="STP_IDENT"/>        
        <property name="urlLogo" length="200"  column="STP_URLCAB"/>        
        <property name="titulo" length="50"  column="STP_TITULO"/>        
        <property name="nombre" length="200" column="STP_NOM"/>     
		<property name="email" length="50"  column="STP_MAIL"/>        
		<property name="password" length="50"  column="STP_PWD"/>        
        <property name="estado" column="STP_ESTADO"/> 
        <property name="horaComienzoEnvio" column="STP_HORAJOB"/>
                       
                   
    </class>
</hibernate-mapping>
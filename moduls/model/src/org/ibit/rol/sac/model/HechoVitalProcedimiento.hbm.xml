<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.HechoVitalProcedimiento" table="RSC_HEVIPR">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column= "HVP_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

        <many-to-one name="hechoVital" column="HVP_CODHEV" foreign-key="RSC_HVPHEV_FK"/>
        <many-to-one name="procedimiento" column="HVP_CODPRO" foreign-key="RSC_HVPPRO_FK"/>
        <property name="orden" column= "HVP_ORDEN"/>

    </class>
</hibernate-mapping>
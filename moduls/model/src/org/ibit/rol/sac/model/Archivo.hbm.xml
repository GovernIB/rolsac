<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="org.ibit.rol.sac.model.Archivo" proxy="org.ibit.rol.sac.model.Archivo" table="RSC_ARCHIV">
        

        <id name="id" unsaved-value="null" column="ARC_CODI" >
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>


        <property name="nombre" length="128" not-null="true" column="ARC_NOMBRE"/>
        <property name="mime" length="128" not-null="true" column="ARC_MIME"/>
        <property name="peso" not-null="true" column="ARC_PESO"/>
        <property name="datos" type="@blob.type@" column="ARC_DATOS"/>
    </class>

</hibernate-mapping>
    <class name="org.ibit.rol.sac.model.UnidadNormativa" table="RSC_UNANOR">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column= "UNN_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

		<many-to-one name="normativa" column="UNN_CODNOR" foreign-key="RSC_UNNNOR_FK"/>
        <many-to-one name="unidadAdministrativa" column="UNN_CODUNA" foreign-key="RSC_UNNUNA_FK"/>


<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.FichaResumenUA" table="RSC_FICHUA" batch-size="100">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="FUA_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>
        <many-to-one name="ficha" column="FUA_CODFIC" foreign-key="RSC_FUAFIC_FK"/>
        <property name="idUa" column="FUA_CODUNA"/>
        <property name="idSeccio" column="FUA_CODSEC"/>
        <property name="orden" column="FUA_ORDEN"/>
        <property name="ordenseccion" column="FUA_ORDSEC"/>
    </class>
</hibernate-mapping>
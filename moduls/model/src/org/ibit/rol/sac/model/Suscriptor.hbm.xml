<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
        
<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.Suscriptor" table="RSC_SCRIPC">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="SCR_COD">
            <generator class="sequence">
                <param name="sequence">RSC_SEQSCR</param>
            </generator>
        </id>
        
        <property name="nombre"  length="100" column="SCR_NOM"/>        
        <property name="apellido1"  length="100" column="SCR_APE1"/>                
        <property name="apellido2"  length="100" column="SCR_APE2"/>                
        <property name="email"  length="100" column="SCR_EMAIL"/>                
        <property name="sms"  length="100" column="SCR_SMS"/>   
        <property name="telefono"  length="100" column="SCR_TELEFONO"/>   
        <property name="tipo"  length="1" column="SCR_TIPO"/>                
        <property name="nombreEntidad"  length="200" column="SCR_ENOM"/>                
        <property name="nombreArea"  length="200" column="SCR_EAREA"/>                        
        <property name="departamentoEntidad"  length="200" column="SCR_EDPTO"/>                
        <property name="cargo"  length="200" column="SCR_ECARGO"/>                
        <property name="boletin"  length="1" column="SCR_IDBOLE"/>                
        <property name="alertas"  length="1" column="SCR_IDALER"/>                
        <property name="estudios"  length="1" column="SCR_IDESTU"/>                
        <property name="estado"  length="1" column="SCR_ESTADO"/>                
        <property name="origen"  length="1" column="SCR_ORIGEN"/>                
        <property name="referenciaTramite"  length="100" column="SCR_REFTRA"/>                
        <property name="usuarioAlta" column="SCR_UALTA"/>                
        <property name="fechaAlta" column="SCR_FCALTA"/>
        <property name="fechaModificacion" column="SCR_FCMOD"/>
        <property name="fechaBaja" column="SCR_FCBAJA"/>
        <property name="sexo" column="SCR_SEXO"/>
        <property name="anyoNacimiento" column="SCR_ANYONAC"/>
        <property name="paisNacimiento" column="SCR_PAISNAC"/>
        <property name="provinciaNacimiento" column="SCR_PROVNAC"/>
        <property name="islaNacimiento" column="SCR_ISLANAC"/>
        <property name="municipioNacimiento" column="SCR_MUNINAC"/>
        <property name="lugarNacimientoOtros" column="SCR_LNACOTROS"/>
        <property name="paisResidencia" column="SCR_PAISRESID"/>
        <property name="provinciaResidencia" column="SCR_PROVRESID"/>
        <property name="islaResidencia" column="SCR_ISLARESID"/>
        <property name="municipioResidencia" column="SCR_MUNIRESID"/>
        <property name="idioma" column="SCR_IDIOMA"/>
        <property name="cif" column="SCR_CIF"/>
        <property name="sedeSocial" column="SCR_SEDESOCIAL"/>
        <property name="razonSocial" column="SCR_RAZONSOCIAL"/>
        <property name="codigoPostal" column="SCR_CP"/>
        <property name="contacto" column="SCR_CONTACTO"/>
        <property name="nombreGrupo" column="SCR_GRNOM"/>
        <property name="resumenTemas" column="SCR_RESUMENTEMAS"/>
        <property name="motivoBaja"  length="200" column="SCR_MOTIVOBAJA"/>                
        
        <many-to-one name="grupoEstudios"  column="SCR_ESTUDIOS" foreign-key="FK_RSC_SCRI_REF_RSC_GRG"/>                
        <many-to-one name="grupoProfesion"  column="SCR_PROFESION" foreign-key="FK_RSC_SCRI_REFERENCE_RSC_GRG"/>                

       
 
        
        <many-to-one name="tipoSuscripcion" not-null="true" column="SCR_STPCOD" foreign-key="RSC_STPSCR_FK"/>                
        <many-to-one name="grupo" column="SCR_SGRCOD" foreign-key="RSC_SGRSCR_FK"/>
        
  		<set name="temas" table="RSC_SCRTEM">
            <cache usage="read-write"/>
            <key column="SCR_COD" foreign-key="FK_RSC_SCRT_REFERENCE_RSC_SCRI"/>
            <composite-element class="org.ibit.rol.sac.model.RegistroTemaSuscriptor">
                <property name="idMateria" column="MAT_CODI"/>
            </composite-element>
        </set>
 

		
        
        
    </class>
</hibernate-mapping>
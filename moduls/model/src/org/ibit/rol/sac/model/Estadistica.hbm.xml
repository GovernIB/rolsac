<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.Estadistica" table="RSC_ESTADI">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="EST_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

        <property name="fecha" column="EST_FECHA"/>
        <property name="contador" column="EST_CONTAD"/>
        <many-to-one name="historico" column="EST_CODHIS" foreign-key="RSC_ESTHIS_FK"/>

    </class>
</hibernate-mapping>
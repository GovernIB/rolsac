<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
        
<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.SuscriptorClave" table="RSC_SCRKEY">
        <cache usage="read-write"/>
        
        <id name="id" unsaved-value="null" column="SCK_COD">
            <generator class="sequence">
                <param name="sequence">RSC_SEQSCK</param>
            </generator>
        </id>
        
        <property name="email"  length="100" column="SCK_EMAIL"/>                
        <property name="imagen"  length="50" column="SCK_IDIMG"/>        
        <property name="activacion"  length="50" column="SCK_IDACT"/>                
        <property name="fechaEnvio" column="SCK_FCENV"/>                
        <property name="numeroEnvio"  column="SCK_NUMENV"/>                
        
        <many-to-one name="tipoSuscripcion" not-null="true" column="SCK_STPCOD" foreign-key="RSC_STPSCK_FK"/>                
        
    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.model.Tratamiento" table="RSC_TRATAM" batch-size="10">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" column="TRT_CODI">
            <generator class="sequence">
                <param name="sequence">RSC_SEQ_ALL</param>
            </generator>
        </id>

        <property name="codigoEstandar" length="128" column="TRT_CODEST" />

        <map name="traducciones" cascade="all" table="RSC_TRATRT">
            <cache usage="read-write"/>
            <key column="TTT_CODTRT" foreign-key="RSC_TTTTRT_FK"/>
            <index column="TTT_CODIDI" type="string" length="2"/>
            <composite-element class="org.ibit.rol.sac.model.TraduccionTratamiento">
                <property name="tipo" length="256" column="TTT_TIPO"/>
                <property name="cargoM" length="256" column="TTT_CARGOM"/>
                <property name="cargoF" length="256" column="TTT_CARGOF"/>
                <property name="tratamientoM" length="256" column="TTT_TRATAM"/>
                <property name="tratamientoF" length="256" column="TTT_TRATAF"/>
            </composite-element>
        </map>
    </class>
</hibernate-mapping>